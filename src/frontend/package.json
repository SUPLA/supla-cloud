  "scripts": {
    "dev": "vite",
    "build": "vite build --emptyOutDir",
    "serve": "vite preview",
    "test:unit": "jest tests",
    "test:e2e:ui": "start-test test:e2e:run-backend :8008/api/server-info dev 5173 'cypress open'",
    "test:e2e:ui:built": "start-test test:e2e:run-backend :8008/api/server-info 'cypress open --config baseUrl=http://localhost:8008'",
    "test:e2e:ci": "start-test test:e2e:run-backend :8008/api/server-info 'cypress run --config baseUrl=http://localhost:8008,defaultCommandTimeout=10000 --spec=tests/e2e/specs/stubbed/*.js,tests/e2e/specs/fullstack/*.js'",
    "test:e2e:run-backend": "cross-env APPLICATION_ENV=e2e php7.4 -S 127.0.0.1:8008 -q -t ../../web",
    "test:e2e:nightly": "cypress run --config baseUrl=https://nightly.supla.org,defaultCommandTimeout=10000 --spec=tests/e2e/specs/nightly/*.js",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore --fix src",
    "collect-translations": "node scripts/collect-translations.js",
    "release": "node scripts/release.js",
    "release-dev": "cross-env NODE_ENV=development node scripts/release.js"
  },
