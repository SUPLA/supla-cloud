    "test:e2e:run-backend": "cross-env APPLICATION_ENV=e2e php8.2 -S 127.0.0.1:8008 -q -t ../../web",
    "test:e2e:nightly": "cypress run --config baseUrl=https://nightly.supla.org,defaultCommandTimeout=10000 --spec=tests/e2e/specs/nightly/*.js",
    "lint": "vue-cli-service lint --mode=production",
    "collect-translations": "node scripts/collect-translations.js",
